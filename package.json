{
  "name": "zillet",
  "version": "0.11.2",
  "description": "Zillet is an open-source, client-side, tool for interacting with the Zilliqa blockchain",
  "author": "man15h <manish9461@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NUXT_HOST=0.0.0.0 NUXT_PORT=7000 nuxt",
    "build": "nuxt build",
    "start": "cross-env NUXT_HOST=0.0.0.0 NUXT_PORT=80 nuxt start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test:unit": "vue-cli-service test:unit",
    "now-build": "nuxt build --spa",
    "https:dev": "node server.js",
    "https:start": "NODE_ENV=production node server.js"
  },
  "keywords": [
    "vue",
    "zilliqa",
    "jazzicon",
    "wallet",
    "nuxt"
  ],
